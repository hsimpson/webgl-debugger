    "prettier": "^2.0.5",
    "raw-loader": "^4.0.1",
    "react-test-renderer": "^16.13.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.2",
    "spectron": "^11.0.0",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "url-loader": "^4.1.0",
