{
  "name": "webgl-debugger",
  "version": "1.0.0",
  "description": "WebGL Debugger Application based on Electron",
  "scripts": {
    "webglProxy": "ts-node ./nodescripts/createWebGLBaseProxy.ts https://www.khronos.org/registry/webgl/specs/latest/1.0/webgl.idl ./src/preload WebGLProxyBase",
    "lint:ts": "eslint ./src/ --ext .ts,.tsx",
    "build:dev": "webpack --config ./config/webpack.dev.ts",
    "build:prod": "webpack --config ./config/webpack.prod.ts",
    "start:dev": "cross-env DEBUG=1 electron .",
    "start:prod": "cross-env electron .",
    "test": "jest --rootDir ./src",
    "test:update-snapshot": "jest --rootDir ./src --updateSnapshot",
    "it": "jest --rootDir ./it"
  },
  "author": "Daniel Toplak <homer.simpson123@gmx.de>",
  "license": "MIT",
  "main": "./dist/app/main.js",
  "bugs": {
    "url": "https://github.com/hsimpson/webgl-debugger/issues"
  },
  "homepage": "https://github.com/hsimpson/webgl-debugger/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hsimpson/webgl-debugger.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "jest-environment-enzyme",
    "transform": {
      "\\.ts(x)?$": "ts-jest"
    }
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/enzyme": "^3.10.5",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^25.2.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.0.5",
    "@types/node-sass": "^4.11.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webdriverio": "^5.0.0",
    "@types/webgl2": "0.0.5",
    "@types/webpack": "^4.41.13",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "@typescript-eslint/typescript-estree": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^10.0.0-beta.1",
    "electron-devtools-installer": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "htmlhint": "^0.13.0",
